      "pages": [
        "wallet/wallet",
        "points/points",
        "withdraw/withdraw",
        "points-mall/points-mall"
      ]
    },
    {
      "root": "subpackages/tasks",
      "pages": [
        "task-list/task-list",
        "task-detail/task-detail",
        "task-publish/task-publish",
        "my-tasks/my-tasks"
      ]
