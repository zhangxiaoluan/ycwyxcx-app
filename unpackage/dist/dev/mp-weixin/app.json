  "window": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "智慧物业",
    "navigationBarBackgroundColor": "#ffffff",
    "backgroundColor": "#ffffff"
  },
  "tabBar": {
    "color": "#515151",
    "selectedColor": "#3b5598",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "height": "55px",
    "fontSize": "12px",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "/static/image/tabBar/home.png",
        "selectedIconPath": "/static/image/tabBar/home-a.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/my/my",
        "iconPath": "/static/image/tabBar/my.png",
        "selectedIconPath": "/static/image/tabBar/my-a.png",
        "text": "我的"
      }
    ]
  },
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "subpackages/user"
      ]
    },
    "pages/login/login": {
      "network": "all",
      "packages": [
        "subpackages/user"
      ]
    }
  },
  "lazyCodeLoading": "requiredComponents",
  "permission": {
    "scope.userLocation": {
      "desc": "您的位置信息将用于安保打卡位置记录"
    }
  },
  "requiredPrivateInfos": [
    "getLocation"
  ],
  "usingComponents": {}
