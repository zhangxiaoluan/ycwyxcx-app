n-grid"><view data-event-opts="{{[['tap',[['goToPayments',['$event']]]]]}}" class="function-card payment" bindtap="__e"><view class="function-icon"><view class="icon-bg payment-bg"><u-icon vue-id="8dd740cc-3" name="coupon" size="24" color="#fff" bind:__l="__l"></u-icon></view></view><text class="function-title">缴费管理</text><text class="function-subtitle">4项费用</text><block wx:if="{{propertyFeeStatus==='overdue'}}"><view class="function-badge"><text>待缴费</text></view></block></view><view data-event-opts="{{[['tap',[['goToRepairs',['$event']]]]]}}" class="function-card repair" bindtap="__e"><view class="function-icon"><view class="icon-bg repair-bg"><u-icon vue-id="8dd740cc-4" name="coupon" size="24" color="#fff" bind:__l="__l"></u-icon></view></view><text class="function-title">报修服务</text><text class="function-subtitle">{{repairStats.pending+"个待处理"}}</text><block wx:if="{{repairStats.pending>0}}"><view class="function-badge"><text>待处理</text></view></block></view><view data-event-opts="{{[['tap',[['goToNotices',['$event']]]]]}}" class="function-card notice" bindtap="__e"><view class="function-icon"><view class="icon-bg notice-bg"><u-icon vue-id="8dd740cc-5" name="bell" size="24" color="#fff" bind:__l="__l"></u-icon></view></view><text class="function-title">公告通知</text><text class="function-subtitle">{{unreadCount+"条未读"}}</text><block wx:if="{{unreadCount>0}}"><view class="function-badge"><text>新消息</text></view></block></view><view data-event-opts="{{[['tap',[['goToConvenience',['$event']]]]]}}" class="function-card convenience" bindtap="__e"><view class="function-icon"><view class="icon-bg convenience-bg"><u-icon vue-id="8dd740cc-6" name="grid" size="24" color="#fff" bind:__l="__l"></u-icon></view></view><text class="function-title">便民服务</text><text class="function-subtitle">6项服务</text></view></view></view><view class="convenience-section"><view class="section-header"><text class="section-title">便民服务</text><text data-event-opts="{{[['tap',[['goToConvenience',['$event']]]]]}}" class="more-text" bindtap="__e">查看全部 ></text></view><view class="service-grid"><block wx:for="{{$root.l1}}" wx:for-item="service" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({service})}}" class="service-item" bindtap="__e"><view class="service-icon"><u-icon vue-id="{{'8dd740cc-7-'+__i0__}}" name="{{service.icon}}" size="24" color="{{service.color}}" bind:__l="__l"></u-icon></view><text class="service-name">{{service.name}}</text></view></block></view></view><view class="notice-preview-section"><view class="section-header"><text class="section-title">社区公告</text><text data-event-opts="{{[['tap',[['goToNotices',['$event']]]]]}}" class="more-text" bindtap="__e">查看全部 ></text></view><view class="notice-preview-list"><block wx:for="{{$root.l2}}" wx:for-item="notice" wx:for-index="__i1__" wx:key="id"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" data-event-params="{{({notice})}}" class="notice-preview-item" bindtap="__e"><view class="{{['notice-dot',(!notice.read)?'unread':'']}}"></view><view class="notice-preview-content"><text class="notice-preview-title">{{notice.title}}</text><text class="notice-preview-time">{{notice.time}}</text></view></view></block></view></view><view class="contact-section"><view class="contact-card"><view class="contact-info"><u-icon vue-id="8dd740cc-8" name="home" size="24" color="#3b5598" bind:__l="__l"></u-icon><view class="contact-details"><text class="contact-title">7x24小时客服</text><text class="contact-phone">400-123-4567</text></view></view><view class="contact-buttons"><button data-event-opts="{{[['tap',[['makePhoneCall',['$event']]]]]}}" class="contact-btn call" bindtap="__e"><u-icon vue-id="8dd740cc-9" name="phone" size="16" color="#fff" bind:__l="__l"></u-icon><text>电话</text></button><button data-event-opts="{{[['tap',[['openChat',['$event']]]]]}}" class="contact-btn chat" bindtap="__e"><u-icon vue-id="8dd740cc-10" name="home" size="16" color="#fff" bind:__l="__l"></u-icon><text>在线</text></button></view></view></view></view>